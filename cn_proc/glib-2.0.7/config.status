s,@host_cpu@,x86_64,;t t
s,@host_vendor@,unknown,;t t
s,@host_os@,linux-gnu,;t t
s,@OS_WIN32_TRUE@,#,;t t
s,@OS_WIN32_FALSE@,,;t t
s,@G_LIB_WIN32_RESOURCE@,,;t t
s,@G_MODULE_WIN32_RESOURCE@,,;t t
s,@G_OBJECT_WIN32_RESOURCE@,,;t t
s,@G_THREAD_WIN32_RESOURCE@,,;t t
s,@GLIB_DEF@,,;t t
s,@GMODULE_DEF@,,;t t
s,@GOBJECT_DEF@,,;t t
s,@GTHREAD_DEF@,,;t t
s,@TESTGMODULE_EXP@,,;t t
s,@ms_librarian@,,;t t
s,@MS_LIB_AVAILABLE_TRUE@,#,;t t
s,@MS_LIB_AVAILABLE_FALSE@,,;t t
s,@ENABLE_GC_FRIENDLY@,,;t t
