s,@oldincludedir@,/usr/include,;t t
s,@infodir@,${prefix}/info,;t t
s,@mandir@,${prefix}/man,;t t
s,@build_alias@,,;t t
s,@host_alias@,,;t t
s,@target_alias@,,;t t
s,@DEFS@,-DHAVE_CONFIG_H,;t t
s,@ECHO_C@,,;t t
s,@ECHO_N@,-n,;t t
s,@ECHO_T@,,;t t
s,@LIBS@, ,;t t
s,@GLIB_MAJOR_VERSION@,2,;t t
s,@GLIB_MINOR_VERSION@,0,;t t
s,@GLIB_MICRO_VERSION@,7,;t t
s,@GLIB_VERSION@,2.0.7,;t t
s,@GLIB_INTERFACE_AGE@,7,;t t
s,@GLIB_BINARY_AGE@,7,;t t
s,@LT_RELEASE@,2.0,;t t
