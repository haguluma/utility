s,@INSTALL_SCRIPT@,${INSTALL},;t t
s,@INSTALL_DATA@,${INSTALL} -m 644,;t t
s,@PACKAGE@,glib,;t t
s,@VERSION@,2.0.7,;t t
s,@ACLOCAL@,/home/general/cn_proc/glib-2.0.7/missing aclocal-1.4,;t t
s,@AUTOCONF@,/home/general/cn_proc/glib-2.0.7/missing autoconf,;t t
s,@AUTOMAKE@,/home/general/cn_proc/glib-2.0.7/missing automake-1.4,;t t
s,@AUTOHEADER@,/home/general/cn_proc/glib-2.0.7/missing autoheader,;t t
